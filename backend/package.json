    "helmet": "^8.1.0",
    "mongoose": "^8.19.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "toobusy-js": "^0.5.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
